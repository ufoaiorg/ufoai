	/**
	 * @param[in] <1> amount of soldiers in the list
	 */
	confunc mp_checkboxes_update {
		*cvar:tmp = <1>
		*cvar:i = 0
		while ( *cvar:i < 8 ) {
			if ( *cvar:i >= *cvar:tmp ) {
				*node:root.bt_actor<cvar:i>@invis = true
			} else {
				*node:root.bt_actor<cvar:i>@invis = false
			}
			*cvar:i = ( *cvar:i + 1 )
		}
		delete *cvar:i
		delete *cvar:tmp
	}

	func mp_checkboxes_set
	{
		*cvar:i = 0
		while ( *cvar:i < 8 ) {
			if ( *node:root.bt_actor<cvar:i>@current != 1 ) {
				call *node:root.bt_actor<cvar:i>@toggle
			}
			cmd "mp_toggleactor <cvar:i> 1;"
			*cvar:i = ( *cvar:i + 1 )
		}
		delete *cvar:i
	}

