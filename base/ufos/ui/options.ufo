	func hidepanels
	{
		*(panel)options_game@invis = true
		*(panel)options_sound@invis = true
		*(panel)options_video@invis = true
		*(panel)options_input@invis = true
		*(panel)options_keys@invis = true
	}

	// ==================
	// buttons
	// ==================

	button button_game
	{
		string		"_Game"
		pos			"80 170"
		size		"256 64"
		image		ui/button_green_big
		font		f_menubig
		color		"0 0.5 0 1"
		selectcolor	"1 1 1 1"
		onClick		{
			call *node:root.hidepanels
			*(panel)options_game@invis = false
		}
	}

