		onClick		{ cmd "mn_pop; copy new_vidmode mn_vidmode;" }
	}
	button bt_cancel
	{
		string		"_Cancel"
		tooltip		"_Close this dialogue"
		color		"0 0.5 0 1"
		font		f_menu
		image		ui/button_green_small
		pos			"297 188"
		size		"176 32"
		onClick		{ cmd "mn_pop; copy new_vidmode old_vidmode;" }
