// ==================
// SEQUENCE MENU
// ==================

window sequence
{
	zone render
	{
		size		"1024 768"
	}

	func next {
		cmd "seq_click;"
	}

	zone click
	{
		size		"1024 768"
		onClick		{ call *node:root.next }
	}

	confunc end
	{
		cmd			"seq_end;"
	}

	func onClose
	{
		call *node:root.end
	}
}
