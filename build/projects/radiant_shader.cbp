<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="uforadiant_shader" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="windows">
				<Option platforms="Windows;" />
				<Option output="..\..\radiant\modules\shader" prefix_auto="1" extension_auto="1" />
				<Option object_output=".objs\radiant_shader" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-Wno-non-virtual-dtor" />
					<Add option="-D__GNUWIN32__" />
					<Add option="-DWINVER=0x501" />
					<Add option="-DNODEBUG" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="--export-all-symbols" />
					<Add library="libglib-2.0" />
				</Linker>
			</Target>
			<Target title="windows_debug">
				<Option platforms="Windows;" />
				<Option output="..\..\radiant\modules\shader" prefix_auto="1" extension_auto="1" />
				<Option object_output=".objs\radiant_shader" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-Wno-non-virtual-dtor" />
					<Add option="-g" />
					<Add option="-D__GNUWIN32__" />
					<Add option="-DWINVER=0x501" />
					<Add option="-DDEBUG" />
					<Add library="libglib-2.0" />
				</Compiler>
				<Linker>
					<Add library="libglib-2.0" />
					<Add option="--export-all-symbols" />
				</Linker>
			</Target>
			<Target title="linux">
				<Option platforms="Unix;" />
				<Option output="..\..\radiant\modules\shader" prefix_auto="1" extension_auto="1" />
				<Option object_output=".objs\radiant_shader" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="`pkg-config glib-2.0 --cflags`" />
					<Add option="-fPIC" />
				</Compiler>
			</Target>
			<Target title="macosx">
				<Option platforms="Mac;" />
				<Option output="..\..\radiant\modules\shader" prefix_auto="1" extension_auto="1" />
				<Option object_output=".objs\radiant_shader" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add directory="\sw\include" />
				</Compiler>
				<Linker>
					<Add directory="\sw\lib" />
					<Add option="-dynamiclib" />
					<Add option="-single_module" />
					<Add option="-compatibility_version 1" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-I../../src/tools/radiant/libs" />
			<Add option="-I../../src/tools/radiant/include" />
		</Compiler>
		<Linker>
			<Add library="libjpeg" />
		</Linker>
		<Unit filename="..\..\src\tools\radiant\plugins\export.def">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="..\..\src\tools\radiant\plugins\shaders\plugin.cpp">
			<Option compilerVar="CC" />
		</Unit>
		<Unit filename="..\..\src\tools\radiant\plugins\shaders\shaders.cpp">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
